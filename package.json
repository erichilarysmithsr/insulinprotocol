{
  "name": "insulinprotocol",
  "version": "1.0.0",
  "description": "Insulin Procotol - Clinical Decision Support Tool",
  "scripts": {
    "start": "node node/www",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js && tsc -p tsconfig.json",
    "prestart_backup": "npm run build",
    "dev": "concurrently \"npm run build:watch\" \"DEBUG=default OAUTH_CLIENT_ID=715165700315-9mhd65j4m68it586c6sph0vl30a7n0a1.apps.googleusercontent.com DATABASE_URL=postgres://dbmanager:secret@localhost:5432/insulindb forever -w --watchDirectory=node node/www\"",
    "dev:node": "concurrently \"npm run build:watch\" \"DEBUG=default OAUTH_CLIENT_ID=715165700315-9mhd65j4m68it586c6sph0vl30a7n0a1.apps.googleusercontent.com DATABASE_URL=postgres://dbmanager:secret@localhost:5432/insulindb node --inspect node/www\"",
    "dev:aot": "OAUTH_CLIENT_ID=715165700315-9mhd65j4m68it586c6sph0vl30a7n0a1.apps.googleusercontent.com DATABASE_URL=postgres://dbmanager:secret@localhost:5432/insulindb NODE_ENV=production node node/www",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "deploy": "npm run build:aot && git push heroku master"
  },
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "keywords": [],
  "author": "Pravin Carvalho",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "~3.4.0",
    "angular2-busy": "^1.0.2",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "crypto": "0.0.3",
    "date-and-time": "^0.4.0",
    "debug": "^2.6.1",
    "express": "~4.14.1",
    "google-auth-library": "^0.10.0",
    "morgan": "~1.7.0",
    "pg-promise": "^5.6.2",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "browser-sync": "^2.18.8",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "connect-browser-sync": "^2.1.0",
    "forever": "^0.15.3",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.5.0",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "rollup": "0.41.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "tslint": "^3.15.1",
    "typescript": "^2.1"
  },
  "repository": {}
}
